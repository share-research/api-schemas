{
   "id":"https://github.com/share-research/api-schemas/datacite.json",
   "$schema":"http://json-schema.org/draft-07/schema#",
   "title":"arXiv Work",
   "description":"The datacite API format for a single work",
   "required":[

   ],
   "type":"object",
   "properties":{
      "identifier":{
         "type":"string"
      },
      "creators":{
         "type":"object",
         "creator":{
            "type":"object",
            "creatorName":{
               "type":"string"
            },
            "givenName":{
               "type":"string"
            },
            "familyName":{
               "type":"string"
            },
            "nameIdentifier":{
               "type":"object",
               "-schemeURI":{
                  "type":"string"
               },
               "-nameIdentifierScheme":{
                  "type":"string"
               },
               "#text":{
                  "type":"string"
               }
            },
            "affiliation":{
               "type":"string"
            }
         }
      },
      "titles":{
         "type":"object",
         "title":[
            {
               "-xml:lang":{
                  "type":"string"
               },
               "#text":{
                  "type":"string"
               }
            },
            {
               "-xml:lang":{
                  "type":"string"
               },
               "-titleType":{
                  "type":"string"
               },
               "#text":{
                  "type":"string"
               }
            }
         ]
      },
      "publisher":{
         "type":"string"
      },
      "publicationYear":{
         "type":"string"
      },
      "subjects":{
         "type":"object",
         "subject":{
            "type":"object",
            "-xml:lang":{
               "type":"string"
            },
            "-schemeURI":{
               "type":"string"
            },
            "-subjectScheme":{
               "type":"string"
            },
            "#text":{
               "type":"string"
            }
         }
      },
      "contributors":{
         "type":"object",
         "contributor":{
            "type":"object",
            "-contributorType":{
               "type":"string"
            },
            "contributorName":{
               "type":"string"
            },
            "nameIdentifier":{
               "type":"object",
               "-schemeURI":{
                  "type":"string"
               },
               "-nameIdentifierScheme":{
                  "type":"string"
               },
               "#text":{
                  "type":"string"
               }
            },
            "affiliation":{
               "type":"string"
            }
         }
      },
      "dates":{
         "type":"object",
         "date":{
            "type":"object",
            "-dateType":{
               "type":"string"
            },
            "#text":{
               "type":"string"
            }
         }
      },
      "language":{
         "type":"string"
      },
      "resourceType":{
         "type":"object",
         "-resourceTypeGeneral":{
            "type":"string"
         },
         "#text":{
            "type":"string"
         }
      },
      "alternateIdentifiers":{
         "type":"object",
         "alternateIdentifier":{
            "type":"object",
            "-alternateIdentifierType":{
               "type":"string"
            },
            "#text":{
               "type":"string"
            }
         }
      },
      "relatedIdentifiers":{
         "type":"object",
         "relatedIdentifier":[
            {
               "-relatedIdentifierType":{
                  "type":"string"
               },
               "-relationType":{
                  "type":"string"
               },
               "-relatedMetadataScheme":{
                  "type":"string"
               },
               "-schemeURI":{
                  "type":"string"
               },
               "#text":{
                  "type":"string"
               }
            },
            {
               "-relatedIdentifierType":{
                  "type":"string"
               },
               "-relationType":{
                  "type":"string"
               },
               "#text":{
                  "type":"string"
               }
            }
         ]
      },
      "sizes":{
         "type":"object",
         "size":{
            "type":"string"
         }
      },
      "formats":{
         "type":"object",
         "format":{
            "type":"string"
         }
      },
      "version":{
         "type":"string"
      },
      "rightsList":{
         "type":"object",
         "rights":{
            "type":"object",
            "-rightsURI":{
               "type":"string"
            },
            "#text":{
               "type":"string"
            }
         }
      },
      "descriptions":{
         "type":"object",
         "description":{
            "type":"object",
            "-xml:lang":{
               "type":"string"
            },
            "-descriptionType":{
               "type":"string"
            },
            "#text":{
               "type":"string"
            }
         }
      },
      "geoLocations":{
         "type":"object",
         "geoLocation":{
            "type":"object",
            "geoLocationPlace":{
               "type":"string"
            },
            "geoLocationPoint":{
               "type":"object",
               "pointLongitude":{
                  "type":"string"
               },
               "pointLatitude":{
                  "type":"string"
               }
            },
            "geoLocationBox":{
               "type":"object",
               "westBoundLongitude":{
                  "type":"string"
               },
               "eastBoundLongitude":{
                  "type":"string"
               },
               "southBoundLatitude":{
                  "type":"string"
               },
               "northBoundLatitude":{
                  "type":"string"
               }
            }
         }
      }
   }
}
